#!/usr/bin/env bash
export CACHE_IMAGE=bossjones/docker-build-speed-test


docker build \
      --target base \
      --cache-from $CACHE_IMAGE:base \
      --tag $CACHE_IMAGE:base \
      --file ./Dockerfile \
      "."


docker push $CACHE_IMAGE:base || true
# docker push $CACHE_IMAGE:stage || true
